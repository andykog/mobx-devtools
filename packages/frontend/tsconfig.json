{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "es5",
    "lib": ["dom", "es2019"],
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib"]
}
